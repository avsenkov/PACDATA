{
  "ui:charts": [
    {
      "type": "lab_series",
      "title": "Лабораторные исследования",
      "selectLabel": "Тип исследования",
      "fromArray": "visits",
      "xField": "visit_date",
      "nestedArray": "hospital_tests",
      "nestedLabelField": "name",
      "nestedValueField": "value_text",
      "includeNames": ["Creatinine (mg/dL)", "AST (U/L)"],
